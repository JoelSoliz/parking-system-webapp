{
  "name": "parking-system-webapp",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write .",
    "lint": "eslint src --max-warnings=0 --ext .js,.jsx --color",
    "lint:fix": "eslint --max-warnings=0 --color --fix",
